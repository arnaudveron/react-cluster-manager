{
  "name": "opensvc-cluster-manager",
  "version": "6.0.0",
  "description": "OpenSVC Cluster Management Frontend.",
  "main": "index.js",
  "config": {
    "mode": "development"
  },
  "scripts": {
    "cleanup": "rm -f src/version.js",
    "pkgversion": "echo \"export const version = \\\"$npm_package_version\\\"\" >> src/version.js",
    "commit": "echo \"export const commit = \\\"$(git log --max-count=1 --pretty=format:%H)\\\"\" >> src/version.js",
    "dumpversion": "npm run cleanup && npm run pkgversion && npm run commit",
    "build": "npm run dumpversion && webpack --mode $npm_package_config_mode"
  },
  "keywords": [],
  "author": "OpenSVC SAS",
  "license": "ISC",
  "dependencies": {
    "@axa-fr/react-oidc-context": "^3.0.2-alpha.0",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/lab": "^4.0.0-alpha.28",
    "autosuggest-highlight": "^3.1.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "eventsource": "^1.0.7",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "i18next": "^17.0.18",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "notistack": "^0.9.5",
    "oidc-client": "^1.9.1",
    "popper.js": "^1.15.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.13.1",
    "react-router-dom": "^5.1.2",
    "react-sparklines": "^1.7.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "typeface-roboto": "0.0.54",
    "url-loader": "^2.0.1",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  }
}
